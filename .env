# Backend API Configuration
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_AUTH_TOKEN_KEY=dreamy_auth_token
REACT_APP_REFRESH_TOKEN_KEY=dreamy_refresh_token

# Feature Flags
REACT_APP_ENABLE_SOCIAL_LOGIN=true
REACT_APP_ENABLE_GUEST_CHECKOUT=true

# Analytics and Monitoring
REACT_APP_ANALYTICS_ID=development
REACT_APP_ERROR_TRACKING_ID=development

# Payment Gateway (if applicable)
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_key
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_sandbox_id

# Cache Configuration
REACT_APP_CACHE_DURATION=3600
REACT_APP_ENABLE_SERVICE_WORKER=false

# Media Configuration
REACT_APP_MEDIA_BASE_URL=http://localhost:8000/media
REACT_APP_MAX_UPLOAD_SIZE=5242880

# Deployment Environment
NODE_ENV=development 